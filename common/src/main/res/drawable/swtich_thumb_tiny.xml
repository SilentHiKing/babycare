<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:bottom="@dimen/dp_2" android:left="0dp" android:right="@dimen/dp_2" android:top="@dimen/dp_2">
                <shape android:shape="oval">
                    <solid android:color="@android:color/white" />
                    <size android:width="@dimen/dp_12" android:height="@dimen/dp_12" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:bottom="@dimen/dp_2" android:left="@dimen/dp_2" android:right="0dp" android:top="@dimen/dp_2">
                <shape android:shape="oval">
                    <solid android:color="@color/White100" />
                    <size android:width="@dimen/dp_12" android:height="@dimen/dp_12" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>

