<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:bottom="3.5dp" android:left="0dp" android:right="3.5dp" android:top="3.5dp">
                <shape android:shape="oval">
                    <solid android:color="@android:color/white" />
                    <size android:width="21dp" android:height="21dp" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:bottom="3.5dp" android:left="3.5dp" android:right="0dp" android:top="3.5dp">
                <shape android:shape="oval">
                    <solid android:color="@color/White100" />
                    <size android:width="21dp" android:height="21dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>



